 {/* class로 component 만들기 */}
      class Modal2 extends React.Component{
        constructor(props){
          super(props);
          this.state = {
            name:"kim",
            age:20
          }
        }
        render(){
          return(
            <div>안녕 {this.state.name}
            <button onClick={()=>{
              this.setState({age:21})
            }}>버튼</button></div>
          )
        }
      }
